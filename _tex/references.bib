  author = {Zakreski, Ellen and Androvi{\v c}ov{\'a}, Ren{\'a}ta and B{\'a}rtov{\'a}, Kl{\'a}ra and Chronos, Agatha and Krej{\v c}ov{\'a}, Lucie and Martinec Nov{\'a}kov{\'a}, Lenka and Klapilov{\'a}, Kate{\v r}ina},
