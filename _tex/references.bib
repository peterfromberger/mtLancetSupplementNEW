@article{jordanSexualInterestSexual2018,
  title = {Sexual Interest and Sexual Self-Control in Men with Self-Reported Sexual Interest in Children -- {{A}} First Eye Tracking Study},
  author = {Jordan, Kirsten and Fromberger, Peter and M{\"u}ller, Isabel and Wernicke, Martina and Stolpmann, Georg and M{\"u}ller, J{\"u}rgen L.},
  year = 2018,
