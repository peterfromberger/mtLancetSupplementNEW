}

@article{sauterImpactTestosteroneloweringMedication2024,
  title = {The Impact of Testosterone-Lowering Medication on Recidivism in Individuals Convicted of Sexual Offenses},
  author = {Sauter, Julia and Lingenti, Laura M. and Rettenberger, Martin and Turner, Daniel and Briken, Peer and Vo{\ss}, Tatjana},
