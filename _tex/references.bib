@article{ristowPedophilicSexOffenders2018,
  title = {Pedophilic Sex Offenders Are Characterised by Reduced {{GABA}} Concentration in Dorsal Anterior Cingulate Cortex},
  author = {Ristow, Inka and Li, Meng and Colic, Lejla and Marr, Vanessa and F{\"o}disch, Carina and {von D{\"u}ring}, Felicia and Schiltz, Kolja and Drumkova, Krasimira and Witzel, Joachim and Walter, Henrik and Beier, Klaus and Kruger, Tillmann H.C. and Ponseti, Jorge and Schiffer, Boris and Walter, Martin},
