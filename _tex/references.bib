  author = {Bowen, Sarah and Witkiewitz, Katie and Clifasefi, Seema L. and Grow, Joel and Chawla, Neharika and Hsu, Sharon H. and Carroll, Haley A. and Harrop, Erin and Collins, Susan E. and Lustyk, M. Kathleen and Larimer, Mary E.},
