  title = {Changes in {{Sexual Arousal}} as {{Measured}} by {{Penile Plethysmography}} in {{Men}} with {{Pedophilic Sexual Interest}}},
  author = {M{\"u}ller, Karolina and Curry, Susan and Ranger, Rebekah and Briken, Peer and Bradford, John and Fedoroff, John Paul},
