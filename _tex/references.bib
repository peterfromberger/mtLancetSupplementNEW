@article{siezengaLookHoodAnalyzing2023,
  title = {A Look under the Hood: Analyzing Engagement and Usage Data of a Smartphone-Based Intervention},
  shorttitle = {A Look under the Hood},
  author = {Siezenga, Aniek M. and Mertens, Esther C. A. and Van Gelder, Jean-Louis},
  year = 2023,
