  shorttitle = {@{{myTabu}}---{{A Placebo Controlled Randomized Trial}} of a {{Guided Web-Based Intervention}} for {{Individuals Who Sexually Abused Children}} and {{Individuals Who Consumed Child Sexual Exploitation Material}}},
  author = {Fromberger, Peter and Schr{\"o}der, Sonja and Bauer, Louisa and Siegel, Bruno and Tozdan, Safiye and Briken, Peer and Buntrock, Claudia and Etzler, Sonja and Rettenberger, Martin and Leha, Andreas and M{\"u}ller, J{\"u}rgen L.},
  year = 2021,
