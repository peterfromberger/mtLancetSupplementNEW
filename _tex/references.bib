@article{dixsonEyeTrackingMens2011,
  title = {Eye {{Tracking}} of {{Men}}'s {{Preferences}} for {{Female Breast Size}} and {{Areola Pigmentation}}},
  author = {Dixson, Barnaby J. and Grimshaw, Gina M. and Linklater, Wayne L. and Dixson, Alan F.},
