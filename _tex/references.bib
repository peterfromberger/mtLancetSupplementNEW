@article{gonzalez-francoRocketboxLibraryUtility2020,
  title = {The {{Rocketbox Library}} and the {{Utility}} of {{Freely Available Rigged Avatars}}},
  author = {{Gonzalez-Franco}, Mar and Ofek, Eyal and Pan, Ye and Antley, Angus and Steed, Anthony and Spanlang, Bernhard and Maselli, Antonella and Banakou, Domna and Pelechano, Nuria and {Orts-Escolano}, Sergio and Orvalho, Veronica and Trutoiu, Laura and Wojcik, Markus and {Sanchez-Vives}, Maria V. and Bailenson, Jeremy and Slater, Mel and Lanier, Jaron},
