@article{naragon-gaineyStructureCommonEmotion2017,
  title = {The Structure of Common Emotion Regulation Strategies: {{A}} Meta-Analytic Examination.},
  shorttitle = {The Structure of Common Emotion Regulation Strategies},
  author = {{Naragon-Gainey}, Kristin and McMahon, Tierney P. and Chacko, Thomas P.},
