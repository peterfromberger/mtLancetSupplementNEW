@article{paquetteStatic99RValidAll2025,
  title = {Is the Static-{{99R}} Valid for All Men with `Identifiable' Victims? {{Examining}} Cases of Online Sexual Solicitation of Children},
  shorttitle = {Is the Static-{{99R}} Valid for All Men with `Identifiable' Victims?},
