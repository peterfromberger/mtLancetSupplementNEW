  author = {Biedermann, J{\"u}rgen and R{\"u}diger, Thomas-Gabriel and Ehlert, Cindy},
  abstract = {The present study deals with the difference between minors and adults accused of child sexual abuse or child pornography offenses who used the internet to commit the offense. For this purpose, different offender types are compared based on information from the police databases of the states of Berlin, Brandenburg, Saxony, and Saxony-Anhalt. A total of 1353 suspects for the sexual abuse of children and 8466 suspects for child pornography offenses could be included in the analyses. The offender types are based on a multidimensional consideration of the offense category in combination with the age of the suspects and victims. The offender types are subsequently characterized in terms of the central features of the offense committed and compared to each other. The analyses show that internet-related sexual offenses committed by minors are associated with different characteristics than those committed by adults. The results are discussed regarding preventive and repressive approaches to the prevention of sexual offenses, whereby the most recent reform of sex offenses with respect to child victims is also subjected to a critical scrutiny.},
  langid = {ngerman},
  file = {/home/frp/Zotero/storage/XV2NYDTK/Biedermann et al. - Die Relevanz von Alterskontexten bei der Präventio.pdf}
}

