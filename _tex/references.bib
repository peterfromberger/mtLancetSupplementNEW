@article{bouchardEmpathyVirtualHumans2013,
  title = {Empathy {{Toward Virtual Humans Depicting}} a {{Known}} or {{Unknown Person Expressing Pain}}},
  author = {Bouchard, St{\'e}phane and Bernier, Fran{\c c}ois and Boivin, {\'E}ric and Dumoulin, St{\'e}phanie and Laforest, Myl{\`e}ne and Guitard, Tanya and Robillard, Genevi{\`e}ve and {Monthuy-Blanc}, Johana and Renaud, Patrice},
  year = 2013,
