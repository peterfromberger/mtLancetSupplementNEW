@article{ferragutEffectivenessChildSexual2023,
  title = {Effectiveness of Child Sexual Abuse Prevention Programs on Knowledge Acquisition: {{A}} Meta-Analytical Study},
  shorttitle = {Effectiveness of Child Sexual Abuse Prevention Programs on Knowledge Acquisition},
  author = {Ferragut, Marta and Cerezo, M. Victoria and {Ortiz-Tallo}, Margarita and {Rodr{\'i}guez-Fernandez}, Raquel},
  year = 2023,
