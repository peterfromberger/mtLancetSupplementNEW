@article{alankoEvidenceHeritabilityAdult2013,
  title = {Evidence for {{Heritability}} of {{Adult Men}}'s {{Sexual Interest}} in {{Youth}} under {{Age}} 16 from a {{Population}}-{{Based Extended Twin Design}}},
  author = {Alanko, Katarina and Salo, Benny and Mokros, Andreas and Santtila, Pekka},
